{
  "name": "coffeegrinder",
  "version": "1.0.0",
  "description": "Yet another frontend for Compiler Explorer",
  "homepage": "https://cooliron2311.github.io/coffeegrinder",
  "scripts": {
    "start": "parcel index.html --no-cache",
    "cleanup": "rm -r .parcel-cache && rm -r dist && rm -r build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "parcel build index.html --dist-dir build --no-source-maps --public-url ./ --no-cache",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "author": "COOLIRON2311",
  "license": "MIT",
  "dependencies": {
    "@monaco-editor/react": "^4.6.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "strip-ansi": "^7.1.0"
  },
  "devDependencies": {
    "@parcel/babel-preset-env": "^2.10.2",
    "eslint": "^8.52.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "gh-pages": "^6.0.0",
    "parcel": "^2.10.2",
    "process": "^0.11.10"
  }
}
